{
    "rewrites": [
        {
            "source": "/supabase/:match*",
            "destination": "https://[YOUR_SUPABASE_DOMAIN_FROM_ENV]/:match*"
        },
        {
            "source": "/arxiv/:match*",
            "destination": "https://export.arxiv.org/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}